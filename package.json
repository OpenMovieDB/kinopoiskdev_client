{
  "name": "@openmoviedb/kinopoiskdev_client",
  "version": "1.3.10",
  "description": "JS/TS для API kinopoisk.dev",
  "main": "./dist/lib.umd.js",
  "module": "./dist/lib.es.js",
  "types": "./dist/types.d.ts",
  "typings": "./dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/lib.umd.js",
      "import": "./dist/lib.es.js",
      "default": "./dist/lib.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start:dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest",
    "release": "release-please --",
    "publish": "npm run build && npm publish --access public"
  },
  "keywords": [
    "kinopoisk",
    "kinopoiskdev",
    "кинопоиск api",
    "api с фильмами",
    "api с сериалами",
    "kinopoisk api"
  ],
  "author": "mdwitr0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenMovieDB/kinopoiskdev_client.git"
  },
  "bugs": {
    "url": "https://github.com/OpenMovieDB/kinopoiskdev_client/issues"
  },
  "homepage": "https://github.com/OpenMovieDB/kinopoiskdev_client#readme",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/browser": "^1.2.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "release-please": "^15.11.0",
    "typescript": "^5.1.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0",
    "webdriverio": "^8.28.8"
  }
}
