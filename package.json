{
  "name": "@openmoviedb/kinopoiskdev_client",
  "version": "1.3.2",
  "description": "JS/TS библиотека для kinopoisk.dev API",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "types": "dist/types.d.ts",
  "module": "dist/module.js",
  "scripts": {
    "start:dev": "parcel watch",
    "build": "parcel build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest",
    "release": "release-please --",
    "publish": "npm run build && npm publish --access public "
  },
  "keywords": [
    "kinopoisk",
    "kinopoiskdev",
    "кинопоиск api",
    "api с фильмами",
    "api с сериалами",
    "kinopoisk api"
  ],
  "author": "mdwitr0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenMovieDB/kinopoiskdev_client.git"
  },
  "bugs": {
    "url": "https://github.com/OpenMovieDB/kinopoiskdev_client/issues"
  },
  "homepage": "https://github.com/OpenMovieDB/kinopoiskdev_client#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.3",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "parcel": "^2.9.3",
    "prettier": "^2.8.8",
    "release-please": "^15.11.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
